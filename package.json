{
  "name": "zero-trust-fullstack",
  "version": "1.0.0",
  "description": "Zero Trust Monitoring System - Full Stack Application",
  "private": true,
  "scripts": {
    "install:all": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
